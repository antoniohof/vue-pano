{
  "name": "vue-pano",
  "description": "WebGL panorama component for vue.js",
  "author": "CodeColorist <zhi.zhou@chaitin.com>",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot",
    "start": "webpack-dev-server --inline --hot --host=0.0.0.0",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "bundle": "cross-env NODE_ENV=github webpack --progress --hide-modules",
    "pages": "./pages.sh",
    "publish": "npm run build"
  },
  "version": "1.3.5",
  "files": [
    "dist/build.js",
    "src/"
  ],
  "keywords": [
    "webgl",
    "browser",
    "panorama"
  ],
  "homepage": "https://chichou.github.io/vue-pano",
  "bugs": "https://github.com/ChiChou/vue-pano/issues",
  "contributors": [
    "CodeColorist"
  ],
  "main": "dist/build.js",
  "license": "MIT",
  "repository": "https://github.com/ChiChou/vue-pano.git",
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "es6-promise": "^4.1.1",
    "vue": "^2.4.2",
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.3",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "json-loader": "^0.5.7",
    "raw-loader": "^0.5.1",
    "url-loader": "^0.5.9",
    "vue-hot-reload-api": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
}
